{
  "name": "ol-tilecache",
  "version": "2.0.0-beta.3",
  "description": "OpenLayers tile url function to load tile seeded with TileCache url scheme",
  "main": "dist/bundle.js",
  "module": "src/index.js",
  "scripts": {
    "build": "cross-env BABEL_ENV=umd webpack --config=build/webpack.umd.config.js",
    "build-min": "cross-env BABEL_ENV=umd NODE_ENV=production webpack --config=build/webpack.umd.config.js",
    "build-all": "npm run build && npm run build-min",
    "start": "webpack-dev-server --config=build/webpack.dev.config.js",
    "test": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "coveralls": "cat test/unit/coverage/lcov.info | coveralls"
  },
  "homepage": "https://github.com/ghettovoice/ol-tilecache",
  "repository": {
    "type": "git",
    "url": "https://github.com/ghettovoice/ol-tilecache.git"
  },
  "keywords": [
    "ol",
    "openlayers",
    "tilecache",
    "ol-tilecache"
  ],
  "author": "Vladimir Vershinin <ghettovoice@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ghettovoice/ol-tilecache/issues"
  },
  "dependencies": {},
  "peerDependencies": {
    "openlayers": ">=3.14"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-latest": "^6.24.0",
    "babel-preset-stage-0": "^6.22.0",
    "chai": "^3.5.0",
    "coveralls": "^2.12.0",
    "cross-env": "^3.2.4",
    "html-webpack-plugin": "^2.28.0",
    "karma": "^1.5.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.30",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.2.0",
    "mocha-phantomjs": "^4.1.0",
    "ol": "^4.0.1-beta.2",
    "openlayers": "^4.0.1",
    "phantomjs-prebuilt": "^2.1.14",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-replace": "^1.1.1",
    "webpack": "^2.3.0",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0",
    "webpack-notifier": "^1.5.0"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "src"
  ],
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  }
}
